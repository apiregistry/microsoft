#%RAML 1.0
title: Engagement.ManagementClient
version: '2014-12-01'
baseUri: management.azure.com
description: Microsoft Azure Mobile Engagement REST APIs.
mediaType:
- application/json
uses:
  commons: https://raw.githubusercontent.com/apiregistry/commons/master/commons.raml
  extras: https://raw.githubusercontent.com/apiregistry/typesExtras/master/typeExtras.raml
securitySchemes:
  azure_auth:
    type: OAuth 2.0
    description: Azure Active Directory OAuth2 Flow
    settings:
      authorizationGrants:
      - implicit
      authorizationUri: https://login.microsoftonline.com/common/oauth2/authorize
      accessTokenUri: https://login.microsoftonline.com/common/oauth2/authorize
      scopes:
      - user_impersonation
types:
  CampaignType:
    type: string
    description: Campaign type.
    enum:
    - Announcement
    - DataPush
    - NativePush
    - Poll
  date:
    type: string
    description: A date as defined by full-date in RFC3339.
  exportFormat:
    type: string
    description: The format of exported data.
    enum:
    - JsonBlob
    - CsvBlob
  exportState:
    type: string
    description: The state of an export task.
    enum:
    - Queued
    - Started
    - Succeeded
    - Failed
  exportType:
    type: string
    description: The type of export.
    enum:
    - Activity
    - Tag
    - Crash
    - Error
    - Event
    - Job
    - Session
    - Token
    - Push
  timestamp:
    type: datetime
    description: A date time as defined by date-time in RFC3339.
  url:
    type: string
  AnnouncementFeedbackCriterion:
