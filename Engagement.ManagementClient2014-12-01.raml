    properties:
      title?:
        type: string
        description: Title of the choice.
        maxLength: 256
  PollQuestionLocalization:
    type: object
    properties:
      title?:
        type: string
        description: Title of the question.
        maxLength: 256
  PushQuotaFilter:
    type: object
    description: Engage only users for whom the push quota is not reached.
