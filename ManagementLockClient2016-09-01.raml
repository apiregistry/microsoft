              description: Gets the management lock of a scope.
              queryParameters:
                api-version:
                  type: string
                  description: Client Api Version.
                  displayName: api-version
              responses:
                200:
                  description: OK
                  body:
                    application/json:
                      type: ManagementLockObject
            delete:
              description: Deletes the management lock of a subscription.
              queryParameters:
                api-version:
                  type: string
                  description: Client Api Version.
                  displayName: api-version
              responses:
                200:
                  description: OK
                204:
                  description: NoContent
            put:
              description: Create or update a management lock at the subscription level.
              queryParameters:
                api-version:
                  type: string
                  description: Client Api Version.
                  displayName: api-version
              body:
                application/json: ManagementLockObject
              responses:
                200:
                  description: OK
                  body:
                    application/json:
                      type: ManagementLockObject
                201:
                  description: Created
                  body:
                    application/json:
                      type: ManagementLockObject
          get:
            description: Gets all the management locks of a subscription.
            queryParameters:
              $filter?:
                type: string
                description: The filter to apply on the operation.
                displayName: $filter
              api-version:
                type: string
                description: Client Api Version.
                displayName: api-version
            responses:
              200:
                description: OK
                body:
                  application/json:
                    type: ManagementLockListResult
            (extras.Pagination):
              nextLinkName: nextLink
