      /{resourceGroupName}:
        uriParameters:
          resourceGroupName:
            type: string
            description: The resource group name uniquely identifies the resource group within the user subscriptionId.
            displayName: resourceGroupName
            minLength: 3
            pattern: '[a-zA-Z0-9]+'
        /providers:
          /Microsoft.ServerManagement:
            /gateways:
              /{gatewayName}:
                uriParameters:
                  gatewayName:
                    type: string
