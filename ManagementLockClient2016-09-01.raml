#%RAML 1.0
title: ManagementLockClient
version: '2016-09-01'
baseUri: management.azure.com
mediaType:
- application/json
uses:
  commons: https://raw.githubusercontent.com/apiregistry/commons/master/commons.raml
  extras: https://raw.githubusercontent.com/apiregistry/typesExtras/master/typeExtras.raml
securitySchemes:
  azure_auth:
    type: OAuth 2.0
    description: Azure Active Directory OAuth2 Flow
    settings:
      authorizationGrants:
      - implicit
      authorizationUri: https://login.microsoftonline.com/common/oauth2/authorize
      accessTokenUri: https://login.microsoftonline.com/common/oauth2/authorize
      scopes:
      - user_impersonation
types:
  ManagementLockListResult:
    type: object
    properties:
      value?:
        type: array
        description: The list of locks.
        items:
          type: ManagementLockObject
      nextLink?:
        type: string
        description: The URL to get the next set of results.
    description: The list of locks.
  ManagementLockObject:
    type: object
    properties:
      properties:
        type: ManagementLockProperties
        description: The properties of the lock.
      id?:
        type: string
        description: The Id of the lock.
      type?:
        type: string
        description: The type of the lock.
      name?:
        type: string
        description: The name of the lock.
    description: Create or update management lock parameters.
  ManagementLockOwner:
    type: object
    properties:
      applicationId?:
        type: string
        description: The application Id of the lock owner.
  ManagementLockProperties:
    type: object
    properties:
      level:
        type: string
        description: The lock level of the lock.
        enum:
        - NotSpecified
        - CanNotDelete
        - ReadOnly
      notes?:
        type: string
        description: The notes of the lock.
      owners?:
        type: array
        description: The owners of the lock.
        items:
          type: ManagementLockOwner
    description: The lock properties.
/subscriptions:
